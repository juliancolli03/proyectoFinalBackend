    <input type="password" class="my-2" id="password" name="password" placeholder="contraseÃ±a a crear" required> <br>
    <input type="text" class="my-2" id="direccion" placeholder="direccion" name="direccion" required> <br>
  
    <input type="number" class="my-2" id="numero" placeholder="(cod pais, cod area, numero)" name="numero" required> <br>
    <input type="file" name="foto" id="foto">
    <input type="number" class="my-2" id="edad" placeholder="edad" name="edad" required> <br>
